# Coding Agent Configuration

# Service settings
CONTAINER_NAME=coding-agent
WEB_PORT=3000

# Ollama settings
OLLAMA_NUM_PARALLEL=2      # Number of parallel requests
OLLAMA_NUM_THREADS=12      # CPU threads to use
OLLAMA_NUM_CTX=32768       # Context window size (tokens)
OLLAMA_KEEP_ALIVE=30m      # How long to keep model in memory

# Resource limits
CPU_LIMIT=14               # CPU cores limit
MEMORY_LIMIT=80G           # RAM limit
CPU_RESERVATION=8          # Minimum CPU cores
MEMORY_RESERVATION=40G     # Minimum RAM

# Security
WEBUI_AUTH=false           # Enable authentication (true/false)
# If WEBUI_AUTH=true, set these:
# WEBUI_SECRET_KEY=your-secret-key-here

# Model settings
DEFAULT_MODEL=qwen2.5-coder:32b-instruct-q5_K_M

# Code execution
ENABLE_CODE_EXECUTION=true
CODE_EXECUTION_SANDBOX=true

# Timezone
TZ=America/Chicago
